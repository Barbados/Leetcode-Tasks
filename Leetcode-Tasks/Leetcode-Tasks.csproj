<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>Leetcode_Tasks</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="BinaryTree\" />
    <None Remove="Array\" />
    <None Remove="String\" />
    <None Remove="CustomLib" />
    <None Remove="dp\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="BinaryTree\" />
    <Folder Include="Array\" />
    <Folder Include="String\" />
    <Folder Include="dp\" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="CustomLib">
      <HintPath>bin\Debug\net6.0\CustomLib.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
